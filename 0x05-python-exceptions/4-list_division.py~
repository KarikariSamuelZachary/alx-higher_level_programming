#!/usr/bin/python3

def list_division(my_list_1, my_list_2, list_length):
    new_list = []
    for i in my_list_1:
        for j in my_list_2:
            try:
                result = i / j
                new_list.append(result)
            except (ValueError):
                result = 0
            except (TypeError):
                print("wrong type")
            except (ZeroDivisionError):
                print("division by 0")
            finally:
                if len(my_list_1) != len(my_list_2):
                    print("out of range")
